{
    "name": "gupalo/bpmnworkflow-bundle",
    "description": "Bpmn Workflow Bundle",
    "type": "symfony-bundle",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\BpmmWorkflowBundle\\Tests\\": "tests/",
            "Gupalo\\BpmnWorkflowBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.11|^3.0",
        "symfony/form": "^6.0",
        "symfony/security-core": "^6.0",
        "symfony/translation": "^6.0",
        "symfony/yaml": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "gupalo/bpmnworkflow": "^1.0",
        "symfony/expression-language": "^6.0",
        "symfony/orm-pack": "^2.2",
        "symfony/asset": "^6.0",
        "symfony/validator": "^6.0",
        "phpunit/phpunit": "^9.5",
        "nelmio/alice": "^3.10",
        "symfony/phpunit-bridge": "^6.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "8.1"
        },
        "sort-packages": true
    }
}
